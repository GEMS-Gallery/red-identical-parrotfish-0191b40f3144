type Task = 
 record {
   categoryId: nat;
   dueDate: text;
   id: nat;
   name: text;
   tag: text;
 };
type Category = 
 record {
   icon: text;
   id: nat;
   name: text;
 };
service : {
  addCategory: (text, text) -> (nat);
  addTask: (text, text, nat, text) -> (nat);
  deleteTask: (nat) -> (bool);
  getCategories: () -> (vec Category) query;
  getTasks: () -> (vec Task) query;
  updateCategory: (nat, text, text) -> (bool);
  updateTask: (nat, text, text, nat, text) -> (bool);
}
